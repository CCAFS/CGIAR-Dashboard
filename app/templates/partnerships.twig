{% set pageParams = {
    'title': 'Partnerships | CGIAR Program Results Dashboard',
    'customCSS': [
      'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css',
      'https://use.fontawesome.com/releases/v5.2.0/css/all.css',
      './css/customInputsFlat.css',
      './css/sideMenu.css',
      './css/global.css',
      './css/partnerships.css'
    ],
    'customJS': [
      "./js/partnerships.js",
      "./js/sideMenu.js"
    ]
  }
%}

{% include './pages/header.twig' with pageParams %}

<div class="container-fluid">
<div class="row justify-content-center">
  <div class="col-md-2">
   {% include './pages/side-menu.twig' with pageParams %}
  </div>
  <div class="col-md-9">
    <!-- Filters -->
    {% if embed %}
      {% include './pages/filters.twig' with pageParams %}
    {% endif %}
  <div class="main-content">

  <button class="navbar-toggler" type="button">
        <span class="navbar-toggler-icon"></span>
  </button>

  {% include './pages/message.twig' %}

  <div class="mb-4">
    <h6 class="section-title">Partnerships</h6>
    <p class="section-description">A Partnership is a recognized relationship between a CRP and another institution/institutions external to CGIAR, to jointly undertake activities that contribute to each institution's mandate. </p>
  </div>

  <!-- Filters inside generated by Javascript -->
  <div class="alert alert-dark selection" style="display:none"></div>

  <div class="col">
      <div class="legend"><div class="legend-title">Research Phases</div> <div class="phases"><div class="square squareuno"></div>Phase 1: Research (Discovery/Proof of Concept)</div>
        <div class="phases"><div class="square squaredos"></div>Phase 2: Piloting</div>
        <div class="phases"><div class="square squaretres"></div>Phase 3/4: Scaling up and scaling out</div>
        <div class="phases"><div class="square squarecuatro"></div>Not provided</div>
      </div>
  </div>
      <div class="form-group row">
          <div class="col-sm-2">
            <div class="top-line"></div>
           <center><h6 class="subTitle">Total Partnerships</h6><br><br><img src="./images/partnerships.svg" width="80%">
            <div class="ptotal-container">
             <div id="total-partnerships"></div>
            </div></center>
          </div>
          <div class="col-md-4">
            <div class="top-line"></div>
            <h6 class="subTitle">Total Partnerships by Research Phase</h6>
            <div class="pstage-container">
              <div id="tp-phase"></div>
            </div>
           </div>
          <div class="col-md-6">
            <div class="top-line"></div>
            <h6 class="subTitle">Key Partnerships Location</h6>
            <div class="ptype-container">
              <div id="partnerships-map"></div>
            </div>
          </div>
          <br><br><br>
      </div>
      <div class="form-group row">
          <div class="col-sm-2">
            <div class="top-line"></div>
            <center><h6 class="subTitle">Key Partnerships</h6><br><br><img src="./images/keypartnershipsicon.svg" width="80%">
            <div class="ptotal-container">
             <div id="total-keyp"></div>
            </div></center>
          </div>
          <div class="col-md-4">
            <div class="top-line"></div>
            <h6 class="subTitle">Key Partnerships by Research Phase</h6>
            <div class="pstage-container">
              <div id="keyp-phase"></div>
            </div>
           </div>
          <div class="col-md-6">
            <div class="top-line"></div>
            <h6 class="subTitle">Key Partnerships by Type</h6>
            <div class="ptype-container">
              <div id="keyp-type"></div>
            </div>
          </div>
          <br><br><br>
      </div>
     <div class="form-group bot-block">
        <div class="top-line"></div>
        <h6 class="subTitle">List of Key External Partnerships</h6>
            <div class="ptable-container">
              <div id="keyp-list"></div>
            </div>

     </div>

{% include './pages/footer.twig'  with pageParams %}
