{% set pageParams = {
    'title': 'Partnerships | CGIAR Program Results Dashboard',
    'customCSS': [
      'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css',
      'https://use.fontawesome.com/releases/v5.2.0/css/all.css',
      './css/customInputsFlat.css',
      './css/sideMenu.css?20200722',
      './css/global.css',
      './css/fulloicr.css?20200727'
    ],
    'customJS': [
      "./js/partnerships-2018.js?20200714",
      "./js/sideMenu.js"
    ]
  }
%}

{% include './pages/header.twig' with pageParams %}

<div
	class="container-fluid">
	<!-- Filters -->
	{% if embed %}
		<div class="row justify-content-center">
			<div class="col-md-2" style="display:{{ displayNav ? 'block' : 'none' }}"></div>
			<div class="col-md-10">{% include './pages/filters.twig' with pageParams %}</div>
		</div>
	{% endif %}
	<div class="row justify-content-center">
		<div class="col-md-2" style="display:{{ displayNav ? 'block' : 'none' }}">
			{% include './pages/side-menu.twig' with pageParams %}
		</div>
		<div class="col-md-10" {% if displayNav==false %} style="padding-left: 0!important" {% endif %}>
			<div class="main-content">

				<button class="navbar-toggler" type="button">
					<span class="navbar-toggler-icon"></span>
				</button>

				{% include './pages/message.twig' %}

				<a class="btn btn-data" href="https://dataverse.harvard.edu/dataset.xhtml?persistentId=doi:10.7910/DVN/KEGEUE" target="_blank" role="button">Access raw data</a>

				<div class="mb-4">
					<h6 class="section-title">Partnerships</h6>
					<p class="section-description">
						A Partnership is a recognized relationship between CGIAR Research Programs and Platforms and another institution/institutions external to CGIAR, to jointly undertake activities that contribute to each institution's mandate. A Partnership can include multiple partners. Partnerships reported here represent a selection of the total number of CGIAR partnerships.
					</p>
				</div>

				<!-- Filters inside generated by Javascript -->
				<div class="alert alert-dark selection" style="display:none"></div>

				<div class="partnerships-charts dashboard-container">
					<div id="all-partnerships"></div>
				</div>


				{% include './pages/footer.twig'  with pageParams %}
