{% set pageParams = {
    'title': 'Contribution to System Level Outcomes | CGIAR Program Results Dashboard',
    'customCSS': [
      'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css',
      'https://use.fontawesome.com/releases/v5.2.0/css/all.css',
      './css/customInputsFlat.css?20200806b',
      './css/sideMenu.css?20210601b',
      './css/global.css?20210601b',
      './css/fulloicr.css?20200806b'
    ],
    'customJS': [
      "./js/systemLevelOutcomesNew.js?20210308b",
      "./js/sideMenu.js?20200806b"
    ]
  }
%}

{% include './pages/header.twig' with pageParams %}

<div class="container-fluid">
  <!-- Filters -->
  {% if embed %}
    <div class="row justify-content-center">
      <div class="col-md-2" style="display:{{ displayNav ? 'block' : 'none' }}"></div>
      <div class="col-md-10">{% include './pages/filters.twig' with pageParams %}</div>
    </div>
  {% endif %}
<div class="row justify-content-center">
  <div class="col-md-2" style="display:{{ displayNav ? 'block' : 'none' }}">
   {% include './pages/side-menu.twig' with pageParams %}
  </div>
  <div class="col-md-10" {% if displayNav==false %}style="padding-left: 0!important"{% endif %}>
    <div class="main-content">

        <button class="navbar-toggler" type="button">
          <span class="navbar-toggler-icon"></span>
        </button>

        {% include './pages/message.twig' %}

        <a class="btn btn-data" href="https://doi.org/10.7910/DVN/RCUWD5" target="_blank" role="button">Access raw data</a>

        <div class="mb-4">
          <h6 class="section-title">Contribution to System Level Outcomes</h6>
          <p class="section-description">SLOs are CGIAR's highest level goals, aligned with the United Nations Sustainable Development Goals (SDGs). To access CGIAR's Strategy and Results Framework 2016-30 click <a href="https://cgspace.cgiar.org/bitstream/handle/10947/3865/CGIAR%20Strategy%20and%20Results%20Framework.pdf" target="_blank">here</a>.</p>
        </div>

        <!-- Filters inside generated by Javascript -->
        <div class="alert alert-dark selection" style="display:none"></div>


          <div class="slo-charts dashboard-container">
              <div id="all-oicr"></div>
          </div>



{% include './pages/footer.twig'  with pageParams %}
