{% set pageParams = {
    'title': 'Progress Towards Milestones | CGIAR Program Results Dashboard',
    'customCSS': [
      'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css',
      'https://use.fontawesome.com/releases/v5.2.0/css/all.css',
      './css/customInputsFlat.css?20200731a',
      './css/sideMenu.css?20200731a',
      './css/global.css?20200731a',
      './css/milestones.css?20200731a'
    ],
    'customJS': [
      "./js/milestones.js?20200731a",
      "./js/sideMenu.js?20200731a"

    ]
  }
%}

{% include './pages/header.twig' with pageParams %}
{% import "./macros/buttons.twig" as buttons %}

<div class="container-fluid">
  <!-- Filters -->
  {% if embed %}
    <div class="row justify-content-center">
      <div class="col-md-2" style="display:{{ displayNav ? 'block' : 'none' }}"></div>
      <div class="col-md-9">{% include './pages/filters.twig' with pageParams %}</div>
    </div>
  {% endif %}
<div class="row justify-content-center">
  <div class="col-md-2" style="display:{{ displayNav ? 'block' : 'none' }}">
  {% include './pages/side-menu.twig' with pageParams %}
  </div>

  <div class="col-md-9">

  <div class="main-content">

    <button class="navbar-toggler" type="button">
        <span class="navbar-toggler-icon"></span>
    </button>

    {% include './pages/message.twig' %}

    <a class="btn btn-data disabled" href="https://doi.org/10.7910/DVN/UZ5TQR" target="_blank" role="button">Access raw data</a>

    <div class="mb-4">
      <h6 class="section-title">Progress Towards Milestones</h6>
    </div>

    <!-- Filters inside generated by Javascript -->
    <div class="alert alert-dark selection" style="display:none"></div>

    <div class="form-group bot-block">
      <div class="data-year"><span class="year-tag badge badge-pill" style="display:none">2018 only</span></div>
        <div class="case-container">
            <div class="d-flex justify-content-center mt-5 mb-5">
              <div class="coming-soon">
                <img src="images/coming-soon.png" width="100%">
              </div>
            </div>
        </div>
{% include './pages/footer.twig'  with pageParams %}
