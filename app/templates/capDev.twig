{% set pageParams = {
    'title': 'Capacity Development | CGIAR Program Results Dashboard',
    'customCSS': [
      'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css',
      'https://use.fontawesome.com/releases/v5.2.0/css/all.css',
      './css/customInputsFlat.css',
      './css/sideMenu.css',
      './css/global.css',
      './css/capDev.css'
    ],
    'customJS': [
      'https://code.jquery.com/jquery-3.3.1.min.js',
      'https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js',
      'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js',
      "./js/libs/tableau/tableau-2.2.2.min.js",
      "./js/libs/sly/sly.min.js",
      "./js/capDev.js?220419",
      "./js/sideMenu.js"
    ]
  }
%}

{% include './pages/header.twig' with pageParams %}

<div class="container-fluid">
<div class="row">
  <div class="col-md-auto">
    {% include './pages/side-menu.twig' with pageParams %}
  </div>
  <div class="col-md-9">
  <div class="main-content">

      <button class="navbar-toggler" type="button">
        <span class="navbar-toggler-icon"></span>
      </button>

      {% include './pages/message.twig' %}
      
      <!-- Filters inside generated by Javascript -->
      <div class="alert alert-dark selection" style="display:none"></div>
      
      <br />
      <br/>
      <div class="form=-grup row">
        <div class="col-md-6">

        <div class="from-group">
          <center>
          <div class="tparticipants" id="totalTrainees"><img src="./images/trainees.svg" height="50px"></div>
          <h6 class="subTitle">Total of Trainees</h6>
          <h6 class="totalsCap">In CGIAR Training Events</h6>
           <div class="totalCapDev d-flex justify-content-center">
            <div id="total-trainees"></div>
          </div>
          </center>
        </div>

        <div class="form-group row">

          <div class="col">
          <center>
          <div class="tparticipants" id="totalWomen"><img src="./images/women.svg" height="50px"></div>
          <h6 class="subTitle">Total of Women</h6>
          <h6 class="totalsCap">Participants and Trainees</h6>
          <div class="totalCapDev">
            <div id="total-women"></div>
          </div>
          </center>
        </div>
        <div class="col">
          <center>
          <div class="tparticipants" id="totalMen"><img src="./images/men.svg" height="50px"></div>
          <h6 class="subTitle">Total of Men</h6>
          <h6 class="totalsCap">Participants and Trainees</h6>
          <div class="totalCapDev">
            <div id="total-men"></div>
          </div>
          </center>
        </div>

        </div>

        </div>
        <div class="col-md-6">
          <h6 class="subTitle">Trainees: Total Women and Men by Training Term</h6>
          <div class="trainees-container">
            <div id="trainees-term"></div>
          </div>
        </div>
      </div>

  </div>
  </div>
</div>
</div>

{% include './pages/footer.twig'  with pageParams %}
